{
    "compilerOptions": {
        "declaration": false,
        "module": "CommonJS",
        "target": "es2018",
        "noImplicitAny": true,
        "noLib": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true
    },
    "exclude": [
        "node_modules"
    ]
}
